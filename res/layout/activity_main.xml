<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#E1E0DE"
    android:id="@+id/fullscreen_content"
    tools:context="com.example.hongyu.myapplication.MainActivity">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="false"
        android:background="@drawable/corners_bg"
        android:fitsSystemWindows="false"
        android:id="@+id/linearLayout">

        <TextView
            android:layout_width ="100dp"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="本局信息:"
            android:textSize="20dp"
            android:textStyle="bold" />
        <TextView
            android:layout_width ="60dp"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="台号:"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/cur_set_table"
            android:layout_width ="30dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="1"
            android:inputType="number"
            android:textSize="20dp"/>
        <TextView
            android:layout_width ="60dp"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="局数:"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/cur_set_number"
            android:layout_width ="30dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="1"
            android:inputType="number"
            android:textSize="20dp" />

        <TextView
            android:layout_width ="100dp"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="开始时间:"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/cur_set_time"
            android:layout_width ="150dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="time"
            android:textSize="20dp" />
        <TextView
            android:layout_width ="70dp"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="跟踪员:"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/cur_set_recorder"
            android:layout_width ="80dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="小明"
            android:textSize="20dp" />
        <TextView
            android:layout_width ="60dp"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="统计:"
            android:textSize="20dp" />
        <TextView
            android:id="@+id/cur_set_gain"
            android:layout_width ="80dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text=""
            android:textSize="20dp" />

        <Button
            android:layout_width ="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="修改"
            android:id="@+id/set_edit"
            android:textSize="20dp"/>
        <Button
            android:layout_width ="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="清空"
            android:id="@+id/set_clean"
            android:textSize="20dp" />
        <Button
            android:layout_width ="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="结束"
            android:id="@+id/set_end"
            android:textSize="20dp" />
        <Button
            android:layout_width ="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_horizontal|center_vertical"
            android:text="历史"
            android:id="@+id/set_history"
            android:textSize="20dp" />

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="150dp"
        android:layout_height="250dp"
        android:layout_below="@+id/linearLayout"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:background="@drawable/corners_bg"
        android:id="@+id/linearLayout2">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal|center_vertical"
            android:text="盘口信息"
            android:id="@+id/textView"
            android:textSize="20dp"
            android:layout_marginBottom="5dp"
            android:textStyle="bold"
            android:layout_marginTop="5dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp">

            <TextView
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="  时间"
                android:id="@+id/textView2"
                android:textSize="20dp"
                android:layout_gravity="center_vertical" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="time"
                android:id="@+id/game_time"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp">

            <TextView
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="  盘口数"
                android:id="@+id/textView4"
                android:textSize="20dp"
                android:layout_gravity="center_vertical" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1"
                android:textSize="20dp"
                android:id="@+id/game_number" />
        </LinearLayout>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal|center_vertical"
            android:text="管理玩家"
            android:id="@+id/player_edit"
            android:textSize="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#dedcd2"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="5dp">
        </TextView>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal|center_vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:text="结果:">
            </TextView>
            <Spinner
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:entries="@array/game_result"
                android:layout_gravity="center"
                android:id="@+id/game_result"></Spinner>
        </LinearLayout>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal|center_vertical"
            android:text="开"
            android:id="@+id/game_end"
            android:textSize="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp" />
    </LinearLayout>

    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:id="@+id/horizontalScrollView"
        android:layout_alignTop="@+id/linearLayout2"
        android:layout_toRightOf="@+id/linearLayout2"
        android:layout_toEndOf="@+id/linearLayout2"
        android:background="@drawable/corners_bg"
        android:layout_marginLeft="5dp" >

